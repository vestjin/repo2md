{
  "name": "repo2md",
  "publisher": "vestjin",
  "displayName": "Repo to Markdown",
  "description": "Convert a folder into a Markdown file with directory tree and file contents",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [                               
    "markdown",
    "export",
    "documentation",
    "directory tree",
    "file content"
  ],
  "icon": "icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "repo2md.start",
        "title": "Repo to Markdown: Select Folder and Generate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.x",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vestjin/repo2md.git"
  },
  "bugs": {                                    
    "url": "https://github.com/vestjin/repo2md/issues"
  },
  "homepage": "https://github.com/vestjin/repo2md", 
  "license": "GPL-3.0-only"  
}
